{
  "name": "@joktec/core",
  "description": "JokTec - Core library",
  "version": "0.2.0",
  "private": false,
  "license": "MIT",
  "author": "JokTec",
  "homepage": "https://github.com/joktec/joktec-monorepo/tree/main/packages/core",
  "repository": {
    "type": "git",
    "url": "https://github.com/joktec/joktec-monorepo.git",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "bin": {
    "client-generator": "bin/client-generator.sh",
    "publish-docker": "bin/publish-docker.sh",
    "publish-helm": "bin/publish-helm.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "restful",
    "graphql",
    "core"
  ],
  "scripts": {
    "clean": "rimraf -rf dist",
    "purge": "rimraf -rf dist node_modules",
    "prebuild": "rimraf -rf dist",
    "build": "nest build -p tsconfig.json",
    "prepublishOnly": "yarn build",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@nestjs*/ -u",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compodoc": "npx compodoc -s"
  },
  "dependencies": {
    "@bull-board/api": "^6.7.10",
    "@bull-board/express": "^6.7.10",
    "@bull-board/ui": "^6.7.10",
    "@joktec/utils": "0.1.1",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/cqrs": "^11.0.2",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/graphql": "^13.0.3",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.0.12",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.0.7",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.0.12",
    "@nestjs/throttler": "^6.4.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "async": "^3.2.6",
    "async-retry": "^1.3.3",
    "body-parser": "^1.20.3",
    "bullmq": "^5.44.0",
    "csurf": "^1.11.0",
    "dataloader": "^2.2.3",
    "express": "^5.0.1",
    "express-basic-auth": "^1.2.1",
    "geoip-lite": "^1.4.10",
    "glob": "^11.0.1",
    "graphql": "^16.10.0",
    "hbs": "^4.2.0",
    "helmet": "^8.1.0",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.2",
    "nestjs-pino": "^4.4.0",
    "opossum": "^8.4.0",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "pino-socket": "^7.4.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "retry": "^0.13.1",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.26",
    "@types/async-retry": "^1.4.9",
    "@types/csurf": "^1.11.5",
    "@types/express": "^5.0.1",
    "@types/geoip-lite": "^1.4.4",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^1.4.12",
    "@types/opossum": "^8.1.8",
    "@types/request-ip": "^0.0.41",
    "@types/swagger-ui-express": "^4.1.8"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "passWithNoTests": true,
    "preset": "ts-jest",
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|s)s$": "ts-jest"
    },
    "coverageDirectory": "<rootDir>/../coverage/",
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/index.ts",
      "!**/*.{d,enum}.ts"
    ]
  },
  "compodoc": {
    "name": "JokTec - Core library",
    "tsconfig": "tsconfig.doc.json",
    "output": "compodoc"
  },
  "gitHead": "5c13d4ee8ca290e9f0f98482f014c818faf98e42"
}
