{
  "name": "@joktec/http",
  "description": "JokTec - Http Client Service",
  "version": "0.0.44",
  "private": false,
  "license": "MIT",
  "author": "JokTec",
  "homepage": "https://github.com/joktec/joktec-monorepo.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/joktec/joktec-monorepo.git",
    "directory": "packages/http"
  },
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "http",
    "api",
    "axios"
  ],
  "scripts": {
    "clean": "rimraf -rf dist",
    "purge": "rimraf -rf node_modules",
    "prebuild": "yarn clean",
    "build": "nest build -p tsconfig.json",
    "prepublishOnly": "yarn build",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@joktec*/ -u"
  },
  "dependencies": {
    "@joktec/core": "0.0.45",
    "axios": "^1.3.5",
    "form-data": "^4.0.0",
    "http-proxy-agent": "^5.0.0",
    "http-status-codes": "^2.2.0",
    "https-proxy-agent": "^5.0.1",
    "lodash": "^4.17.21",
    "merge-deep": "^3.0.3",
    "qs": "^6.11.0",
    "retry-axios": "^2.3.0",
    "rxjs": "^7.2.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/merge-deep": "^3.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHead": "ec04871b4a33b5b377c641eec801c14e4f8dafe3"
}
