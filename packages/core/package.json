{
  "name": "@joktec/core",
  "description": "JokTec - Core library",
  "version": "0.0.128",
  "private": false,
  "license": "MIT",
  "author": "JokTec",
  "homepage": "https://github.com/joktec/joktec-monorepo/tree/main/packages/core",
  "repository": {
    "type": "git",
    "url": "https://github.com/joktec/joktec-monorepo.git",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "bin": {
    "client-generator": "bin/client-generator.sh",
    "publish-docker": "bin/publish-docker.sh",
    "publish-helm": "bin/publish-helm.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "restful",
    "graphql",
    "core"
  ],
  "scripts": {
    "clean": "rimraf -rf dist",
    "purge": "rimraf -rf dist node_modules",
    "prebuild": "rimraf -rf dist",
    "build": "nest build -p tsconfig.json",
    "prepublishOnly": "yarn build",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@nestjs*/ -u",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compodoc": "npx compodoc -s"
  },
  "dependencies": {
    "@bull-board/api": "^5.8.2",
    "@bull-board/express": "^5.8.2",
    "@bull-board/ui": "^5.8.2",
    "@grpc/proto-loader": "^0.7.9",
    "@nestjs/bull": "^10.1.0",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/testing": "^10.3.3",
    "@nestjs/throttler": "^5.1.2",
    "@willsoto/nestjs-prometheus": "5.3.0",
    "async": "^3.2.4",
    "async-retry": "^1.3.3",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dataloader": "^2.2.2",
    "dot-object": "^2.1.5",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "geoip-lite": "^1.4.7",
    "graphql": "^16.8.0",
    "hbs": "^4.2.0",
    "helmet": "^7.0.0",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "merge-deep": "^3.0.3",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nestjs-pino": "^4.0.0",
    "opossum": "^8.1.1",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "pino-socket": "^7.3.0",
    "pluralize": "^8.0.0",
    "prom-client": "^14.2.0",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "retry": "^0.13.1",
    "rxjs": "^7.8.1",
    "slug": "^8.2.3",
    "swagger-ui-express": "^5.0.0",
    "ua-parser-js": "^1.0.36",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@types/async-retry": "^1.4.5",
    "@types/bcryptjs": "^2.4.3",
    "@types/csurf": "^1.11.3",
    "@types/express": "^4.17.17",
    "@types/geoip-lite": "^1.4.1",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.198",
    "@types/merge-deep": "^3.0.0",
    "@types/multer": "^1.4.7",
    "@types/opossum": "^8.1.2",
    "@types/pluralize": "^0.0.30",
    "@types/qs": "^6.9.8",
    "@types/request-ip": "^0.0.38",
    "@types/slug": "^5.0.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/ua-parser-js": "^0.7.37",
    "@types/url-parse": "^1.4.9",
    "@types/uuid": "^9.0.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "passWithNoTests": true,
    "preset": "ts-jest",
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|s)s$": "ts-jest"
    },
    "coverageDirectory": "<rootDir>/../coverage/",
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/index.ts",
      "!**/*.{d,enum}.ts"
    ]
  },
  "compodoc": {
    "name": "JokTec - Core library",
    "tsconfig": "tsconfig.doc.json",
    "output": "compodoc"
  },
  "gitHead": "0b74c0ca619607d15d0e21c6ae6adf6b2abca3da"
}
