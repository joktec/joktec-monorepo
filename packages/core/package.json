{
  "name": "@joktec/core",
  "description": "JokTec - Core library",
  "version": "0.0.76",
  "private": false,
  "license": "MIT",
  "author": "JokTec",
  "homepage": "https://github.com/joktec/joktec-monorepo/tree/main/packages/core",
  "repository": {
    "type": "git",
    "url": "https://github.com/joktec/joktec-monorepo.git",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "bin": {
    "client-generator": "bin/client-generator.sh",
    "publish-docker": "bin/publish-docker.sh",
    "publish-helm": "bin/publish-helm.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "restful",
    "graphql",
    "core"
  ],
  "scripts": {
    "clean": "rimraf -rf dist",
    "purge": "rimraf -rf node_modules",
    "prebuild": "rimraf -rf dist",
    "build": "nest build -p tsconfig.json",
    "prepublishOnly": "yarn build",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@joktec*/ -u",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@bull-board/api": "^5.2.1",
    "@bull-board/express": "^5.2.1",
    "@bull-board/ui": "^5.2.1",
    "@grpc/proto-loader": "^0.7.3",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/graphql": "^11.0.5",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/testing": "^9.4.0",
    "@nestjs/throttler": "^4.0.0",
    "@willsoto/nestjs-prometheus": "^5.1.2",
    "async": "^3.2.0",
    "async-retry": "^1.3.3",
    "backoff": "^2.5.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csurf": "^1.11.0",
    "dataloader": "^2.1.0",
    "dot-object": "^2.1.4",
    "express": "^4.18.2",
    "express-useragent": "^1.0.15",
    "graphql": "^16.6.0",
    "hbs": "^4.2.0",
    "helmet": "^7.0.0",
    "i18n": "^0.15.1",
    "js-yaml": "3.14.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "merge-deep": "^3.0.3",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nestjs-pino": "^3.2.0",
    "opossum": "^6.2.1",
    "pino": "^8.14.1",
    "pino-cloudwatch": "^0.7.0",
    "pino-fluentd": "^0.2.4",
    "pino-http": "^8.3.3",
    "pino-loki": "^2.1.3",
    "pino-pretty": "^10.0.0",
    "pino-stackdriver": "^3.0.0",
    "pluralize": "^8.0.0",
    "prom-client": "^13.0.0",
    "qs": "^6.7.0",
    "reflect-metadata": "^0.1.13",
    "retry": "^0.13.1",
    "rxjs": "^7.2.0",
    "slug": "^8.2.2",
    "swagger-ui-express": "^4.6.2",
    "type-cacheable": "^4.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.17",
    "@types/express-useragent": "^1.0.2",
    "@types/helmet": "^4.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/i18n": "^0.13.6",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.185",
    "@types/merge-deep": "^3.0.0",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@types/pluralize": "^0.0.29",
    "@types/qs": "^6.5.3",
    "@types/slug": "^5.0.3",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/url-parse": "^1.4.8"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "passWithNoTests": true,
    "preset": "ts-jest",
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|s)s$": "ts-jest"
    },
    "coverageDirectory": "<rootDir>/../coverage/",
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/index.ts",
      "!**/*.{d,enum}.ts"
    ]
  },
  "gitHead": "25fef554d74349f4b06e8da1628fd3dd73069196"
}
