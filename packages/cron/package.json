{
  "name": "@joktec/cron",
  "description": "JokTec - Cron Service",
  "version": "0.0.27",
  "private": false,
  "license": "MIT",
  "author": "JokTec",
  "homepage": "https://github.com/joktec/joktec-monorepo/tree/main/packages/cron",
  "repository": {
    "type": "git",
    "url": "https://github.com/joktec/joktec-monorepo.git",
    "directory": "packages/cron"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "schedule",
    "cron",
    "mysql",
    "async",
    "cargoQueue"
  ],
  "scripts": {
    "clean": "rimraf -rf dist",
    "purge": "rimraf -rf node_modules",
    "dev": "rollup -cw",
    "prebuild": "yarn clean",
    "build": "nest build -p tsconfig.json",
    "prepublishOnly": "yarn build",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@joktec*/ -u"
  },
  "dependencies": {
    "@joktec/core": "0.0.26",
    "@joktec/mysql": "0.0.27",
    "@nestjs/schedule": "^2.2.0",
    "async": "^3.2.0",
    "async-retry": "^1.3.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
