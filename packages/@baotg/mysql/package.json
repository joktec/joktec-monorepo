{
  "name": "@baotg/mysql",
  "description": "JobHopin - MySql Service",
  "version": "1.0.20",
  "private": false,
  "license": "UNLICENSED",
  "author": "BaoTran",
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "purge": "rm -rf node_modules",
    "dev": "nest start --watch -p tsconfig.json",
    "prebuild": "yarn clean",
    "build": "nest build -p tsconfig.json",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@baotg*/ -u",
    "gen:entity": "./bin/generate-entity.sh && yarn format && yarn lint"
  },
  "dependencies": {
    "@baotg/core": "1.0.20",
    "knex": "^2.3.0",
    "knex-schema-inspector": "^3.0.0",
    "lodash": "^4.17.21",
    "mysql": "2.18.1",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/mysql": "^2.15.17"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHead": "b74397663527dd2bd67714cddaefe03c1b9112d2"
}
