{
  "name": "@baotg/minio",
  "description": "JobHopin - MinIO Service",
  "version": "0.0.0",
  "private": false,
  "license": "UNLICENSED",
  "author": "BaoTran",
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "purge": "rm -rf node_modules",
    "dev": "rollup -cw",
    "prebuild": "yarn clean",
    "build": "nest build -p tsconfig.json",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@baotg*/ -u"
  },
  "dependencies": {
    "@baotg/core": "1.0.20",
    "image-downloader": "^4.3.0",
    "merge-deep": "^3.0.3",
    "minio": "^7.0.32",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/merge-deep": "^3.0.0",
    "@types/minio": "^7.0.15"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
