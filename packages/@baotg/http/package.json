{
  "name": "@baotg/http",
  "description": "JobHopin - Http Client Service",
  "version": "1.0.20",
  "private": false,
  "license": "UNLICENSED",
  "author": "BaoTran",
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "purge": "rm -rf node_modules",
    "dev": "rollup -cw",
    "prebuild": "yarn clean",
    "build": "nest build -p tsconfig.json",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@baotg*/ -u"
  },
  "dependencies": {
    "@baotg/core": "1.0.20",
    "@nestjs/axios": "^1.0.1",
    "axios": "^1.2.1",
    "form-data": "^4.0.0",
    "http-proxy-agent": "^5.0.0",
    "http-status-codes": "^1.3.2",
    "https-proxy-agent": "^5.0.1",
    "lodash": "^4.17.21",
    "merge-deep": "^3.0.3",
    "retry-axios": "^3.0.0",
    "rxjs": "^7.2.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/merge-deep": "^3.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHead": "b74397663527dd2bd67714cddaefe03c1b9112d2"
}
