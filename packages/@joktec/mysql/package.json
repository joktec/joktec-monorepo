{
  "name": "@joktec/mysql",
  "description": "JokTec - MySql Service",
  "version": "0.0.3",
  "private": false,
  "license": "MIT",
  "author": "JokTec",
  "homepage": "https://github.com/joktec/joktec-monorepo/tree/main/packages/%40joktec/mysql",
  "repository": {
    "type": "git",
    "url": "https://github.com/joktec/joktec-monorepo.git",
    "directory": "packages/%40joktec/mysql"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mysql",
    "mysql2",
    "sequelize",
    "sequelize-typescript"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "purge": "rm -rf node_modules",
    "dev": "nest start --watch -p tsconfig.json",
    "prebuild": "yarn clean",
    "build": "nest build -p tsconfig.json",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@joktec*/ -u",
    "gen:entity": "./bin/generate-entity.sh && yarn format && yarn lint"
  },
  "dependencies": {
    "@joktec/core": "0.0.1",
    "lodash": "^4.17.21",
    "mysql": "2.18.1",
    "mysql2": "^3.1.2",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.17",
    "@types/validator": "^13.7.12"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHead": "b74397663527dd2bd67714cddaefe03c1b9112d2"
}
