{
  "name": "@joktec/elastic",
  "description": "JokTec - ElasticSearch",
  "version": "1.0.20",
  "private": false,
  "license": "MIT",
  "author": "JokTec",
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "purge": "rm -rf node_modules",
    "dev": "rollup -cw",
    "prebuild": "yarn clean",
    "build": "nest build -p tsconfig.json",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@joktec*/ -u"
  },
  "dependencies": {
    "@joktec/core": "1.0.20",
    "@elastic/elasticsearch": "^8.5.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/elasticsearch": "^9.0.0",
    "axios": "^1.2.1",
    "lodash": "^4.17.21",
    "merge-deep": "^3.0.3",
    "retry-axios": "^3.0.0",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@types/merge-deep": "^3.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-path ./.prettierignore",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHead": "b74397663527dd2bd67714cddaefe03c1b9112d2"
}
